; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NovoJogo"
config/description="Web3 Survival Game inspired by Rust"
config/version="0.2.6"
run/main_scene="res://scenes/ui/main_menu.tscn"
config/features=PackedStringArray("4.6", "GL Compatibility")

[autoload]

BuildingManager="*res://scripts/building/building_manager.gd"
NetworkManager="*res://scripts/networking/network_manager.gd"
WorldGenerator="*res://scripts/world/world_generator.gd"
ChunkManager="*res://scripts/world/chunk_manager.gd"
SFXGenerator="*res://scripts/audio/sfx_generator.gd"
GameSettings="*res://scripts/autoload/game_settings.gd"
WeaponSfx="*res://scripts/audio/weapon_sfx.gd"
CombatNetcode="*res://scripts/networking/combat_netcode.gd"
MatchManager="*res://scripts/gamemode/match_manager.gd"
NetworkMetrics="*res://scripts/networking/network_metrics.gd"
MobileInput="*res://scripts/autoload/mobile_input.gd"
MobilePerformance="*res://scripts/autoload/mobile_performance.gd"
AutoUpdater="*res://scripts/autoload/auto_updater.gd"
FootstepSfx="*res://scripts/audio/footstep_sfx.gd"
CombatSfx="*res://scripts/audio/combat_sfx.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1

[dotnet]

project/assembly_name="NovoJogo"

[editor_plugins]

enabled=PackedStringArray("res://addons/gut/plugin.cfg")

